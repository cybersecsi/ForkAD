{
  "name": "forkad",
  "author": "Angelo Delicato <angelo.delicato@secsi.io>",
  "private": true,
  "version": "0.1.2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
